{
  "name": "tattoo",
  "version": "2.0.0-alpha.1",
  "description": "Test all the things over and over",
  "main": "lib/runner.js",
  "bin": {
    "tattoo": "lib/bin/tattoo.js"
  },
  "scripts": {
    "format": "find src custom_typings | grep '\\.ts$' | xargs clang-format --style=file -i",
    "test": "tslint src/*.ts src/*/*.ts custom_typings/*.ts; mocha",
    "build": "tsc",
    "tattoo": "node lib/bin/tattoo.js",
    "build:watch": "tsc -w",
    "test:watch": "watch 'npm test' ./",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PolymerLabs/tattoo.git"
  },
  "keywords": [
    "bot"
  ],
  "author": "The Polymer Project Authors",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/PolymerLabs/tattoo/issues"
  },
  "dependencies": {
    "bottleneck": "^1.10.2",
    "bower": "^1.7.7",
    "command-line-args": "^3.0.3",
    "command-line-usage": "^3.0.7",
    "dom5": "^1.3.1",
    "escape-string-regexp": "^1.0.5",
    "github": "^0.2.0",
    "nodegit": "^0.16.0",
    "pad": "^1.0.0",
    "progress": "^1.1.8",
    "promisify-node": "^0.3.0",
    "resolve": "^1.1.7",
    "rimraf": "^2.4.3",
    "semver": "^5.1.0"
  },
  "devDependencies": {
    "@types/chai": "^3.4.29",
    "@types/mocha": "^2.2.28",
    "@types/progress": "^1.1.8",
    "@types/rimraf": "^0.0.28",
    "@types/semver": "^5.1.0",
    "chai": "^2.2.0",
    "chai-diff": "^1.0.1",
    "clang-format": "=1.0.45",
    "mocha": "^2.2.4",
    "strip-ansi": "^3.0.1",
    "tslint": "^3.10.2",
    "typescript": "^2.0.3",
    "watch": "^0.17.1"
  },
  "optionalDependencies": {
    "source-map-support": "^0.4.0"
  }
}
